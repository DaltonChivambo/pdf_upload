{
  "name": "upload-pdf-backend",
  "version": "1.0.0",
  "description": "Backend para upload de PDFs com PostgreSQL e TypeScript",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "init-db": "ts-node src/database/init.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.5.0",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.2",
    "@types/cors": "^2.8.13",
    "typescript": "^5.1.6",
    "ts-node-dev": "^2.0.0"
  }
}